{
  "ver" : "1.0.0",
  "authors" : ["tgu2"],
  "contact" : "help@kbase.us",
  "visible" : true,
  "categories" : ["active","util"],
  "widgets" : {
    "input" : null,
    "output" : "no-display"
  },
   "parameters" : [
  {
    "id" : "matrix_ref",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : ["KBaseFeatureValues.ExpressionMatrix", "KBaseMatrices"]
    }
  },{
   "id" : "k_num",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "validate_as": "float"
    }
  },{
    "id" : "cluster_set_name",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
        "valid_ws_types" : ["KBaseExperiments.ClusterSet"],
        "is_output_name" : true
      }
  }],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "kb_clustering",
      "method" : "run_kmeans_cluster",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property" : "workspace_name"
        },
        {
          "input_parameter" : "matrix_ref",
          "target_type_transform": "resolved-ref",
          "target_property" : "matrix_ref"
        },
        {
            "input_parameter" : "k_num",
            "target_property" : "k_num"
        },
        {
            "input_parameter" : "cluster_set_name",
            "target_property" : "cluster_set_name"
        }],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
         "service_method_output_path": [0,"cluster_set_refs"],
         "target_property": "cluster_set_refs"
        },
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        },
        {
          "constant_value": "16",
          "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field" : "docker"
}
